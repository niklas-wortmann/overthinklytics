{
  "name": "django-backend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "django-backend/django_backend",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": [
        "{projectRoot}/dist"
      ],
      "options": {
        "command": "cd apps/django-backend && uv build"
      },
      "cache": true
    },
    "serve": {
      "command": "cd apps/django-backend && uv run manage.py runserver",
      "cache": true
    },
    "lint": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "cd apps/django-backend && uv run ruff check django_backend"
      },
      "cache": true
    },
    "format": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "cd apps/django-backend && uv run ruff format django_backend"
      },
      "cache": true
    },
    "test": {
      "command": "cd apps/django-backend && uv run python manage.py test",
      "outputs": [
        "{workspaceRoot}/reports/{projectRoot}/unittests",
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "cache": true
    }
  },
  "tags": []
}
